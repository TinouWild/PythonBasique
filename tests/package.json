{
  "name": "Backup",
  "version": "4.1",
  "author": "Sauvegarde",
  "description": "Backup Frontend",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "dist": "build --win --x64",
    "dist:win32": "build --win --ia32",
    "dist:linux_x64": "build --linux --x64",
    "dist:mac": "build --mac",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js"
  },
  "build": {
    "productName": "Backup",
    "appId": "com.backup.gui",
    "directories": {
      "output": "build/v4/beta"
    },
    "publish": {
      "provider": "generic",
      "url": "http://upd.sauvegardes.org/v4-custom/kit41",
      "channel": "master"
    },
    "files": [
      "dist/electron",
      "node_modules/",
      "package.json"
    ],
    "extraFiles": [
      {
        "from": "static.zip",
        "to": "./Resources/.",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "certificateSubjectName": "KIWI BACKUP",
      "target": "nsis",
      "icon": "resources/kitzip/icon.ico",
      "publisherName": "Kiwi Backup",
      "verifyUpdateCodeSignature": false,
      "requestedExecutionLevel": "requireAdministrator",
      "extraFiles": [
        {
          "from": "server/dist/",
          "to": ".",
          "filter": [
            "**/*"
          ]
        },
      {
        "from": "config.json",
        "to": ".",
        "filter": [
          "**/*"
        ]
      }
      ]
    },
    "nsis": {
      "perMachine": true,
      "allowElevation": true,
      "menuCategory": true
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "dist_files/icons/custom/icon.icns",
      "identity": "K3MN8ZTV58",
      "extraFiles": [
        {
          "from": "../server/dist-mac/",
          "to": ".",
          "filter": [
            "**/*"
          ]
        },
      {
        "from": "../server/config.json",
        "to": "./Resources/.",
        "filter": [
          "**/*"
        ]
      }
      ]
    },
    "linux": {
      "icon": "dist_files/icons/custom",
      "target": "deb",
      "category": "backup system",
      "extraFiles": [
        {
          "from": "../server/dist-linux/",
          "to": ".",
          "filter": [
            "**/*"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "6": "0.0.1",
    "builder-util": "^5.8.1",
    "fcopy-pre-bundled": "^0.3.4",
    "is-ci": "^1.1.0",
    "os": "0.1.1",
    "read-config-file": "^3.0.1",
    "request-promise-retry": "^1.0.0",
    "sudo-prompt": "^8.2.0",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-resource": "^1.5.0",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1",
    "yargs": "^11.1.0",
    "request-promise": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.2.0",
    "babili-webpack-plugin": "^0.1.1",
    "bootstrap": "^3.3.7",
    "cfonts": "^1.2.0",
    "chai": "^4.0.0",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "^9.1.0",
    "electron-builder": "^19.56.2",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^2.2.14",
    "electron-updater": "^2.21.10",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.16.1",
    "inject-loader": "^3.0.0",
    "jquery": "^3.3.1",
    "json": "^9.0.6",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "require-dir": "^0.3.0",
    "spectron": "^3.8.0",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.7",
    "v-folder": "file:./local_node_modules/v-folder",
    "vue-html-loader": "^1.2.2",
    "vue-i18n": "^6.1.1",
    "vue-loader": "^12.2.1",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.11.2",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.2"
  }
}
